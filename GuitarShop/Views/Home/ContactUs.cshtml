@{
    ViewData["Title"] = "Contact Us";
}
<h1>Contact Us</h1>

<p>Here's how to you can reach us:</p>

<table class="table table-bordered table-hover">
    <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>@item.Key</td>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>
</table>